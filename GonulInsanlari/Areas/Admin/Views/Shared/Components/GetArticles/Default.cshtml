@model List<Article>

<div class="col-lg-6">
    <div class="ibox ">
        <div class="ibox-title">
            <h5><a href="/Admin/Article/List" style="color:inherit">Articles</a></h5>
            <div class="ibox-tools">
                <a class="collapse-link">
                    <i class="fa fa-chevron-up"></i>
                </a>
                <a class="close-link">
                    <i class="fa fa-times"></i>
                </a>
            </div>
        </div>
        <div class="ibox-content table-responsive">
            <table class="table table-hover no-margins">
                <thead>
                    <tr>
                        <th>#</th>
                        <th>Title</th>
                        <th>Category</th>
                        <th>Published</th>
                    </tr>
                </thead>
                <tbody>


                    @foreach (var article in Model)
                    {

                        <tr style="cursor:pointer">
                          
                            <td><small>#</small></td>
                            <td><a href="/Admin/Article/GetDetails/@article.ArticleID">@article.Title</a></td>
                            <td><span class="label label-info">@article.Category.Name</span></td>
                            <td><i style="padding-right:5px;" class="fa fa-clock-o"></i>@article.Created.ToString("MMM-dd-yyyy")</td>
                        </tr>
                        
                    }

                </tbody>
            </table>
        </div>
    </div>
</div>
