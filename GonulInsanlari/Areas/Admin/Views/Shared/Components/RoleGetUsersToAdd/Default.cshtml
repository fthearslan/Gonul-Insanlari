




<div class="modal inmodal fade" id="newUser" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-md">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">&times;</span><span class="sr-only">Close</span></button>
                <h4 class="modal-title">Add a new user</h4>
            </div>
            <div id="inputLocater" class="modal-body">

                <div class="full-height-scroll">


                    <div class="table-responsive">
                        <table class="table table-striped table-hover">
                            <thead>
                                <tr>
                                    <th><i class="fa fa-user"></i></th>
                                    <th>Username</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody >


                                    <tr>
                                        <td class="client-avatar"><img alt="image" src="~/Images/"></td>
                                        <td></td>
                                        <td>
                                            <input type="checkbox" class="i-checks" value="" />
                                        </td>
                                    </tr>


                                


                            </tbody>
                        </table>
                    </div>

                </div>

                <div id="btnLocater" class="modal-footer">
                    <button type="button" id="btnSave" class="btn btn-primary" onclick="addUser('@ViewData["RoleId"]')"> Save </button>

                </div>
            </div>
        </div>
    </div>
</div>
