@model List<Notification>

<li class="dropdown">
    <a class="dropdown-toggle count-info" data-toggle="dropdown" href="#">
        <i class="fa fa-bell"></i>  <span class="label label-primary">@ViewBag.Count</span>
    </a>
    <ul class="dropdown-menu dropdown-alerts">
        @foreach(var notify in Model){
        <li>
                <a href="/Admin/Notification/GetDetails/@notify.ID" class="dropdown-item">
                <div>
                    <i class="@notify.Symbol"></i>@notify.Title
                    <span class="float-right text-muted small">@GetDate.GetCreateDate(notify.Created)</span>
                </div>
            </a>
        </li>
        <li class="dropdown-divider"></li>
        }
        <li>
            <div class="text-center link-block">
                <a href="notifications.html" class="dropdown-item">
                    <strong>See Other @ViewBag.Rest Notifications</strong>
                    <i class="fa fa-angle-right"></i>
                </a>
            </div>
        </li>
    </ul>
</li>